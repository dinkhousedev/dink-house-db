{
  "name": "dink-house-api",
  "version": "1.0.0",
  "description": "Supabase API for Dink House Database",
  "main": "api/index.js",
  "scripts": {
    "start": "node api/index.js",
    "dev": "nodemon api/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "db:migrate": "./migrate.sh",
    "db:seed": "node api/scripts/seed.js",
    "admin:create": "node api/cli/create-admin.js",
    "players:delete": "node api/cli/delete-all-players.js",
    "players:delete:dry": "node api/cli/delete-all-players.js --dry-run",
    "players:delete:cloud": "node api/cli/delete-all-players.js --cloud",
    "players:delete:cloud:dry": "node api/cli/delete-all-players.js --cloud --dry-run",
    "crowdfunding:reset": "node scripts/reset-crowdfunding.js",
    "crowdfunding:reset:db": "node scripts/reset-crowdfunding.js --db-only",
    "crowdfunding:reset:stripe": "node scripts/reset-crowdfunding.js --stripe-only",
    "crowdfunding:reset:dry": "node scripts/reset-crowdfunding.js --dry-run"
  },
  "dependencies": {
    "@sendgrid/mail": "^8.1.6",
    "@supabase/supabase-js": "^2.58.0",
    "bcrypt": "^5.1.1",
    "commander": "^14.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.1",
    "express-validator": "^7.2.1",
    "helmet": "^7.2.0",
    "inquirer": "^12.9.6",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "nodemailer": "^7.0.6",
    "pg": "^8.11.3",
    "stripe": "^14.11.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supabase": "^2.45.5",
    "supertest": "^6.3.3"
  },
  "keywords": [
    "supabase",
    "api",
    "postgresql",
    "database",
    "dink-house"
  ],
  "author": "Dink House",
  "license": "MIT",
  "directories": {
    "doc": "docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dynamic3dPM/dink-house-db.git"
  },
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/Dynamic3dPM/dink-house-db/issues"
  },
  "homepage": "https://github.com/Dynamic3dPM/dink-house-db#readme"
